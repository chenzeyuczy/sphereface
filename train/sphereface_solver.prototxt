net: "./train/sphereface_model.prototxt"
#test_iter: 100 
#test_interval: 2000

# base_lr: 0.1
# lr_policy: "multistep"
base_lr: 0.1
lr_policy: "step"
gamma: 0.1

#stepvalue: 16000
#stepvalue: 32000
#stepvalue: 56000
#max_iter: 56000
stepsize: 10000
max_iter: 50000

display: 100
momentum: 0.9
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "result/sphereface_model_ms10w"

solver_mode: GPU
